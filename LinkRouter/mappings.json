{
  "profiles": {
    "Firefox": {
      "browser": "C:\\Program Files\\Mozilla Firefox\\firefox.exe",
      "argsTemplate": "\"{url}\""
    },
    "FirefoxNewWindow": {
      "browser": "C:\\Program Files\\Mozilla Firefox\\firefox.exe",
      "argsTemplate": "-new-window \"{url}\""
    },
    "Chrome": {
      "browser": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
      "argsTemplate": "\"{url}\"",
      "workingDirectory": "C:\\Program Files\\Google\\Chrome\\Application"
    },
    "ChromeNewWindow": {
      "browser": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
      "argsTemplate": "-new-window \"{url}\"",
      "workingDirectory": "C:\\Program Files\\Google\\Chrome\\Application"
    }
  },
  "default": {
    "useProfile": "Firefox"
  },
  "rules": [
    {
      "match": "domain",
      "pattern": "google.at",
      "useProfile": "Firefox"
    },
    {
      "match": "domain",
      "pattern": "youtube.com",
      "useProfile": "Chrome"
    },
    {
      "match": "domain",
      "pattern": "pandocs.atlassian.net",
      "useProfile": "Chrome"
    },
    {
      "match": "domain",
      "pattern": "yolo.atlassian.net",
      "useProfile": "Firefox"
    }
  ]
}
